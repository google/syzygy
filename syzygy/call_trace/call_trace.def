; Export declarations for the Tracer DLL.
LIBRARY call_trace.dll

EXPORTS
  ; Our profile hook function.
  _penter

  ; API
  wait_til_enabled
  wait_til_disabled

  ; And the indirect profile hook function we
  ; invoke through machine instrumentation.
  _indirect_penter


  ; Functions we expose to rundll32.
  StartCallTrace
  StopCallTrace
